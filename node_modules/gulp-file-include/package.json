  },
  "homepage": "https://github.com/coderhaoxin/gulp-file-include",
  "devDependencies": {
    "gulp": "*",
    "istanbul": "*",
    "markdown": "*",
    "mocha": "*",
    "should": "*"
  },
  "dependencies": {
